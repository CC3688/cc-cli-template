{
  "name": "vite-vue3-ts-admin",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "build:production": "vue-tsc --noEmit && vite build --mode production",
    "build:preview": "vue-tsc --noEmit && vite build --mode preview",
    "serve:production": "vite preview",
    "serve:preview": "cross-env mode=preview vite preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.js",
    "mock": "json-server --watch --port 5000 ./mock/db.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.4",
    "axios": "^0.23.0",
    "echarts": "^5.2.2",
    "element-plus": "^1.1.0-beta.19",
    "js-cookie": "^3.0.1",
    "json-server": "^0.17.0",
    "md5": "^2.3.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.0",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.10.3",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.19.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.1",
    "postcss-nesting": "^8.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "svgo": "^2.8.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-components": "^0.13.3",
    "vite-plugin-svg-icons": "^1.1.0",
    "vue-tsc": "^0.3.0"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --cache --fix",
    "*.{ts,vue,md}": "prettier --write"
  }
}
